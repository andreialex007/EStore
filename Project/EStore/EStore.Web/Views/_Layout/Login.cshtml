
<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en" class="no-js">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@(ViewBag.Title) - EStore</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <script src="@Url.Content("~/Content/assets/global/plugins/jquery.min.js")" type="text/javascript"></script>
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css" />
    @RenderSection("Styles", false)
    @Styles.Render("~/styles/metronic-mandatory")
    @Styles.Render("~/styles/metronic-page-level")
    @Styles.Render("~/styles/metronic-admin-theme")
    @Styles.Render("~/Content-css");
    @Scripts.Render("~/javascript/metronic-core-scripts")
    @Scripts.Render("~/javascript/metronic-admin-page-level-scripts")
    <!-- BEGIN PAGE LEVEL STYLES -->
    <link href="~/Content/assets/admin/pages/css/login2.css" rel="stylesheet" />
    <!-- END PAGE LEVEL SCRIPTS -->
    <script>
        
        $(function () {
            Metronic.init();
            Layout.init();
            Demo.init();
            Index.init();
        });
    </script>
    @RenderSection("Scripts", false)
</head>
<!-- END HEAD -->
<!-- BEGIN BODY -->
<body class="login">
    <!-- BEGIN LOGO -->
    <div class="logo">
        <img src="~/Content/Images/logo.png" alt="" />
    </div>
    <!-- END LOGO -->
    <!-- BEGIN LOGIN -->
    <div class="content">
        @RenderBody()

    </div>
    <div class="copyright">
        @DateTime.Now.Year
        &copy;
        EStore
    </div>
</body>
<!-- END BODY -->
</html>
